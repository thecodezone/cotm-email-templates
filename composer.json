{
    "name": "cotm/email-templates",
    "description": "Provides Blades, HTML and Twig versions of Church on the Move email templates, along with helpful blade components and includes for use in building emails.",
    "type": "craft-plugin",
    "authors": [
        {
            "name": "Craig Wann",
            "email": "craig@codezone.io"
        }
    ],
    "support": {
        "email": "email@developer-website.tld",
        "issues": "https://bitbucket.org/cotmtulsa/email-templates-v2/jira?statuses=new&statuses=indeterminate&sort=-updated&page=1",
        "source": "https://bitbucket.org/cotmtulsa/email-templates-v2",
        "docs": "https://bitbucket.org/cotmtulsa/email-templates-v2"
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload": {
        "psr-4": {
            "Cotm\\EmailTemplates\\": "src/"
        }
    },
    "require-dev": {
        "tightenco/jigsaw": "^1.3",
        "illuminate/mail": "v5.8.35",
        "craftcms/cms": "^3.4"
    },
    "extra": {
        "name": "Email Templates",
        "handle": "email-templates",
        "laravel": {
            "providers": [
                "Cotm\\EmailTemplates\\EmailTemplatesServiceProvider"
            ]
        }
    }
}
