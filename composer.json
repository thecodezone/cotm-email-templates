{
    "name": "cotm/email-templates",
    "description": "Provides Blades and HTML versions of Church on the Move email templates, along with helpful blade components and includes for use in building emails.",
    "type": "library",
    "authors": [
        {
            "name": "Craig Wann",
            "email": "craig@codezone.io"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload": {
        "psr-4": {
            "Cotm\\EmailTemplates\\": "src/"
        }
    },
    "require-dev": {
        "samdark/sitemap": "^2.2"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/incraigulous/jigsaw"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Cotm\\EmailTemplates\\EmailTemplatesServiceProvider"
            ]
        }
    },
    "require": {
        "tightenco/jigsaw": "dev-master#8ac8e65a32da04b62b38788c347928ee59a5686d"
    }
}
